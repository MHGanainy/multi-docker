{
    "AWSEBDockerrunVersion": 2,
    "containerDefinitions": [
        {
            "name": "client",
            "image": "571998/multi-client",
            "hostname": "client",
            "essential": false,
            "memory": 128
        },
        {
            "name": "server",
            "image": "571998/multi-server",
            "hostname": "api",
            "essential": false,
            "memory": 128
        },
        {
            "name": "worker",
            "image": "571998/multi-worker",
            "hostname": "worker",
            "essential": false,
            "memory": 128
        },
        {
            "name": "nginx",
            "image": "571998/multi-nginx",
            "hostname": "nginx",
            "essential": true,
            "portMappings":[{
                "hostPort": 80,
                "containerPort":80
            }],
            "links": ["server","links"],
            "memory": 128
        }
    ]
    
}